@model EcommerceMVC.ViewModels.CartLabelQuantity
@{
    ViewData["Title"] = "Quantity";
}
<a asp-action="Index" asp-controller="Cart" class="btn px-0 ml-3">
    <i class="fas fa-shopping-cart text-primary"></i>
    <span class="badge text-secondary border border-secondary rounded-circle" style="padding-bottom: 2px;">@Model.Quantity</span>
</a>

